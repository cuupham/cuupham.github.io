    "content": ".venv\\Scripts\\activate\n.venv\\Scripts\\activate.bat"
  },
  {
    "title": "Freeze pip",
    "content": "pip freeze > requirements.txt"
  },
  {
    "title": "Uninstall packages from file",
    "content": "pip uninstall -r requirements.txt -y"
  },
  {
    "title": "Upgrade pip",
    "content": "python -m pip install --upgrade pip"
  },
  {
    "title": "Install packages from file",
    "content": "pip install -r requirements.txt"
  },
  {
    "title": "Install ensurepip",
    "content": "python -m ensurepip --upgrade"
